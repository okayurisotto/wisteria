{
	"name": "wisteria",
	"version": "2024.2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/okayurisotto/wisteria.git"
	},
	"packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
	"workspaces": [
		"packages/frontend",
		"packages/backend",
		"packages/sw",
		"packages/misskey-js",
		"packages/misskey-reversi",
		"packages/misskey-bubble-game"
	],
	"private": true,
	"scripts": {
		"build-pre": "node ./scripts/build-pre.js",
		"build-assets": "node ./scripts/build-assets.mjs",
		"build": "pnpm build-pre && turbo build && pnpm build-assets",
		"build-storybook": "pnpm --filter frontend build-storybook",
		"build-misskey-js-with-types": "pnpm build-pre && pnpm --filter backend... --filter=!misskey-js build && pnpm --filter backend generate-api-json && ncp packages/backend/built/api.json packages/misskey-js/generator/api.json && pnpm --filter misskey-js update-autogen-code && pnpm --filter misskey-js build && pnpm --filter misskey-js api",
		"start": "cd packages/backend && node ./built/boot/entry.js",
		"start:test": "cd packages/backend && cross-env NODE_ENV=test node ./built/boot/entry.js",
		"init": "pnpm migrate",
		"migrate": "cd packages/backend && pnpm migrate",
		"revert": "cd packages/backend && pnpm revert",
		"migrateandstart": "pnpm migrate && pnpm start",
		"watch": "pnpm dev",
		"dev": "node scripts/dev.mjs",
		"lint": "pnpm -r lint",
		"cy:open": "pnpm cypress open --browser --e2e --config-file=cypress.config.ts",
		"cy:run": "pnpm cypress run",
		"e2e": "pnpm start-server-and-test start:test http://localhost:61812 cy:run",
		"jest": "cd packages/backend && pnpm jest",
		"jest-and-coverage": "cd packages/backend && pnpm jest-and-coverage",
		"test": "pnpm -r test",
		"test-and-coverage": "pnpm -r test-and-coverage",
		"clean": "node ./scripts/clean.js",
		"clean-all": "node ./scripts/clean-all.js",
		"cleanall": "pnpm clean-all"
	},
	"resolutions": {
		"chokidar": "3.5.3",
		"lodash": "4.17.21"
	},
	"devDependencies": {
		"@eslint/js": "^9.7.0",
		"@tsconfig/node-lts": "^20.1.3",
		"@tsconfig/strictest": "^2.0.5",
		"@types/eslint__js": "^8.42.3",
		"cross-env": "7.0.3",
		"cssnano": "6.0.3",
		"cypress": "13.6.3",
		"eslint": "^9.7.0",
		"execa": "8.0.1",
		"fast-glob": "3.3.2",
		"ignore-walk": "6.0.4",
		"js-yaml": "4.1.0",
		"ncp": "2.0.0",
		"postcss": "8.4.33",
		"start-server-and-test": "2.0.3",
		"tar": "6.2.0",
		"terser": "5.27.0",
		"turbo": "^2.0.7",
		"typescript": "5.5.4",
		"typescript-eslint": "^7.17.0"
	}
}
