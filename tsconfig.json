{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"isolatedModules": true,
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true
	},
	"exclude": ["built"],
	"extends": ["@tsconfig/strictest/tsconfig.json"]
}
